<h2>Listing Users</h2>

<table class="table">
  <thead>
    <tr>
      <th>id</th>
      <th>Username</th>

      <th></th> 
    </tr>
  </thead>
  <tbody>
    <%= for user <- @users do %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.username %></td>
    </tr>
<% end %>
  </tbody>
</table>

<%= link "New user", to: user_path(@conn, :new) %>